{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@auth/*": ["./src/app/auth/*"],
      "@auth": ["src/app/auth/auth.module.ts"],
      "@training/*": ["./src/app/training/*"],
      "@training": ["src/app/training/training.module.ts"],
      "@onboarding/*": ["./src/app/onboarding/*"],
      "@onboarding": ["src/app/onboarding/onboarding.module.ts"],
      "@interview/*": ["./src/app/interview/*"],
      "@interview": ["src/app/interview/interview.module.ts"],
      "@questionnaire/*": ["./src/app/questionnaire/*"],
      "@questionnaire": ["src/app/questionnaire/questionnaire.module.ts"],
      "@core/*": ["./src/app/core/*"],
      "@core": ["src/app/core/core.module.ts"],
      "@shared/*": ["./src/app/shared/*"],
      "@shared": ["src/app/shared/shared.module.ts"],
      "@ui-components/*": ["./src/app/shared/components/*"],
      "@ui-components": ["src/app/shared/components/index.ts"],
      "@translations/*": ["./src/libs/translations/*"],
      "@translations": ["src/libs/translations/translations.module.ts"],
      "@assets/*": ["./assets/*"],
    }
  },
  "typeRoots": ["./src/app/shared/types"],
  "types": [
    "styled-components-react-native",
    "jest"
  ]
}
